<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Feedback | Report</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <style>
      .big_container{
        margin-top: 3%;
      }
    </style>
</head>
<body>
    <div class="big_container">
    <div class="container">

      <h2 style="text-align: center; color: rgb(100, 100, 100); font-family: sans-serif; padding-bottom: 1%;">Feedbacks</h2>
    <table class="table table-striped">
        <thead>
          <tr>
            <!-- <th scope="col">ID</th> -->
            <th scope="col">Name</th>
            <th scope="col">E-mail</th>
            <th scope="col">Website</th>
            <th scope="col">Feedbacks</th>
          </tr>
        </thead>
        <tbody>
            <%feedbacks1List.forEach(feedbacks => {%>
                <tr>
                    <!-- <th scope="row"><%= feedbacks._id %></th> -->
                    <td><%= feedbacks.name %></td>
                    <td><%= feedbacks.email %></td>
                    <td><%= feedbacks.website %></td>
                    <td><%= feedbacks.comment %></td>
                </tr>
            <%})%>
        </tbody>
      </table>
      <div class="col-sm-offset-5 col-sm-2 text-center pb-10">
        <a class="btn btn-primary float-right" href="/back">&larr;BACK</a>
        </div>
    </div>
  </div>
</body>
</html>